<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Outer circle shape -->
    <item>
        <shape android:shape="oval">
            <solid android:color="#33D6D5EE" />
        </shape>
    </item>

    <!-- Inner circle shape -->
    <item
        android:left="50dp"
        android:top="50dp"
        android:right="50dp"
        android:bottom="50dp">
        <shape android:shape="oval">
            <solid android:color="#F8F8FF" />
        </shape>
    </item>
</layer-list>
